{
  "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:prettier/recommended", "prettier/react"],
  "plugins": ["import", "react", "prettier"],
  "env": {
    "es6": true
  },
  "parserOptions": {
    "ecamaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
